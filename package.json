{
    "name": "nestjs-localization-module",
    "version": "1.1.0",
    "description": "A NestJS module for localization with PostgreSQL integration, category management, and auto-translation support",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm run build",
        "start": "node dist/main.js",
        "start:dev": "tsc-watch -p tsconfig.json --onSuccess \"node dist/example/main.js\"",
        "start:watch": "nodemon --exec ts-node src/main.ts",
        "start:example": "ts-node example/main.ts",
        "test:api": "ts-node example/test-api.ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "prepublish": "npm run build"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "author": "pqcuong737",
    "license": "MIT",
    "dependencies": {
        "@nestjs/common": "^9.0.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/typeorm": "^9.0.0",
        "axios": "^1.8.4",
        "class-transformer": "^0.5.0",
        "class-validator": "^0.14.1",
        "pg": "^8.0.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.5.5",
        "typeorm": "^0.3.0",
        "uuid": "^9.0.0"
    },
    "peerDependencies": {
        "@nestjs/common": "^9.0.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "pg": "^8.0.0"
    },
    "devDependencies": {
        "@types/node": "^22.13.9",
        "@types/uuid": "^9.0.2",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^4.0.0"
    }
}
